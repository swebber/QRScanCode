    <div u="slides" style="cursor: move; position: absolute; width: 960px; height: 380px; top: 0px; left: 0px; overflow: hidden;">
        <div><img u="image" src="~/img/better/better-001.gif" /></div>
        <div><img u="image" src="~/img/better/better-002.png" /></div>
        <div><img u="image" src="~/img/better/better-003.jpg" /></div>
        <div><img u="image" src="~/img/better/better-004.jpg" /></div>
        <div><img u="image" src="~/img/better/better-005.jpg" /></div>
        <div><img u="image" src="~/img/better/better-006.jpg" /></div>
        <div><img u="image" src="~/img/better/better-007.png" /></div>
        <div><img u="image" src="~/img/better/better-008.png" /></div>
    </div>
    <!-- Direction Navigator Skin Begin -->
    <!-- Arrow Left -->
    <span u="arrowleft" class="jssord20l" style="width: 55px; height: 55px; top: 123px; left: 8px;"></span>
    <!-- Arrow Right -->
    <span u="arrowright" class="jssord20r" style="width: 55px; height: 55px; top: 123px; right: 8px"></span>
    <!-- Direction Navigator Skin End -->
</div>

@section scripts
{
    <script src="~/Scripts/slideshow-common.js"></script>
